{
  "extends": [
    "eslint:recommended", 
    "plugin:import/recommended",
    "prettier",
    "next/core-web-vitals"
  ],
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "plugins": ["prettier"],
  "root": true,
  "rules": {
    "no-unused-vars":"warn",
    "prettier/prettier": "warn",
    "react/react-in-jsx-scope": "off",
    "react/display-name": 1
  }
}